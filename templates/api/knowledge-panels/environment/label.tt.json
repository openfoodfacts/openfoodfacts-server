{
    "parent_panel_id": "ecoscore",
    "level" :"info",
    "topics": [
        "environment"
    ],
    "evaluation": "good",
    "title_element": {
        "title": "[% display_taxonomy_tag("labels",panel.label) %]",
        [% IF panel.icon_url.defined %]
            "icon_url": "[% panel.icon_url %]",
        [% END %]
        [% IF panel.environmental_benefits.defined %]
            "subtitle": "[% panel.environmental_benefits %]",
        [% END %]
    },
    [% IF panel.description.defined %]
    "elements": [
        {
            "element_type": "text",
            "text_element": {
                "type": "default",
                "html": `
                    [% panel.description %]
                    `
            }
        }, 
    ],
    [% END %]
}
