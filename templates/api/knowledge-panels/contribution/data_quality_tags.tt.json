{
    "level": "info",
    "topics": [
        "contribution"
    ],
    [%# only expand errors and info %]
    [% IF panel.tags_type == "data_quality_errors" %]
        "expand_for": "large",
    [% ELSIF panel.tags_type == "data_quality_info" %]
        "expand_for": "large",
    [% ELSE %]
        "expanded": false,
    [% END %]
    [% IF panel.tags_type == "data_quality_errors" %]
        "evaluation": "bad",
    [% ELSIF panel.tags_type == "data_quality_warnings" %]
        "evaluation": "average",
    [% ELSE %]
        "evaluation": "good",
    [% END %]
    "title_element": {
        "title": "[% lang(panel.tags_type _ '_panel_title') %]",
        "subtitle": "[% lang(panel.tags_type _ '_panel_subtitle') %]",
        "icon_url": "[% static_subdomain %]/images/icons/dist/healing.svg",
        "icon_color_from_evaluation": true,
    },
    "elements": [
        [% 
            # we need description_key variable because we can't use ':' in key directly
            description_key = 'description:en'
            action_name = "action_$fix_action"
        %]
        [% FOREACH pair IN panel.quality_tags.pairs %]
            [%
                fix_action = pair.key
            %]
            {
                "element_type": "text",
                "text_element": {
                    "html": `[% FILTER collapse %]
                        <ul>
                        [% FOREACH action_pair IN pair.value -%]
                            <li id="data-error-[% action_pair.key %]">[% action_pair.value.$description_key %]</li>
                        [%- END %]
                        </ul>
                    [% END %]`
                },
            },
            [% IF fix_action != 'undef' %]
                {
                    "element_type": "action",
                    "action_element": {
                        "html": `[% lang(action_name) %]`,
                        "actions": [
                            "[% fix_action %]",
                        ]
                    },
                },
            [% END %]
        [% END %]
    ]
}
