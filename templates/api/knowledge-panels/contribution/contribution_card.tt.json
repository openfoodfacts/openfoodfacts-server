[% A panel with information useful to contributors %]

[% IF panels.quality_panels %]
{
    "type": "card",
    "expanded": true,
    "topics": [
        "contribution"
    ],
    "title_element": {
        "title": "[% lang('contribution_panel_title') %]",
    },
    "elements": [
        [% FOREACH panel IN panels.quality_panels %]
        {
            "element_type": "panel",
            "panel_element": {
                "panel_id": "[% panel %]",
            },
        },
        [% END %]
    ],
}
[% END %]