msgstr "صف کشیده"

msgctxt "minion_status_active"
msgid "In progress"
msgstr "در حال انجام"

msgctxt "minion_status_finished"
msgid "Finished"
msgstr "تمام شده"

msgctxt "minion_status_failed"
msgid "Failed"
msgstr "شکست خورده"

